<SigningMethods>
<!-- All signing methods supported in the e-justice portal.-->
	
	<!-- Smart card-->
	<SigningMethod>
		<category>1</category>
		<needsJavaScript>true</needsJavaScript>
		<needsJava>true</needsJava>
		<code>sc</code>
		<jspPage>dynform_dss_applet.jsp</jspPage>		
	</SigningMethod>

	<!-- External (off-line) signature -->
	<SigningMethod>
		<category>1</category>
		<needsJavaScript>false</needsJavaScript>
		<needsJava>false</needsJava>
		<code>ext</code>
		<jspPage>dynform_dss_external.jsp</jspPage>
	</SigningMethod>

	<!-- Austrian Mobile Phone signature -->
	<SigningMethod>
		<category>2</category>	
		<needsJavaScript>false</needsJavaScript>
		<needsJava>false</needsJava>
		<code>ampss</code>
		<jspPage>dynform_dss_ampss.jsp</jspPage>
		<parameter>
			<key>IFRAME_W</key>
			<value>600</value>
		</parameter>
		<parameter>
			<key>IFRAME_H</key>
			<value>800</value>
		</parameter>
		<parameter>
			<key>IFRAME_W</key>
			<value>800</value>
		</parameter>
		<parameter>
			<key>EOSS_PARSER_CLASS</key>
			<value>eu.europa.ejusticeportal.dss.controller.eoss.ampss.AmpssParser</value>
		</parameter>
		<parameter>
			<key>FORM_ACTION</key>
			<value>https://test1.a-trust.at/mobile/https-security-layer-request/default.aspx</value>
		</parameter>
		<parameter>
			<key>ENC_TYPE</key>
			<value>application/x-www-form-urlencoded</value>
		</parameter>
		<parameter>
			<key>XML_REQUEST_NAME</key>
			<value>XMLRequest</value>
		</parameter>
		<parameter>
			<key>XML_RESPONSE_NAME</key>
			<value>XMLResponse</value>
		</parameter>
		<parameter>
			<key>REDIRECT_URL</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/dynform_epo_a_signing_1_action.do?method=ampss&amp;mode=REDIRECT&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@</value>
		</parameter>
		<parameter>
			<key>REDIRECT_URL_TESTPAGE</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/dynform_signing_testpage_action.do?method=ampss&amp;mode=REDIRECT&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@</value>
		</parameter>
		<parameter>
			<key>REDIRECT_URL_NO_JS</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/dynform_signing_eoss_action.do?method=ampss&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@&amp;mode=WAIT</value> 
		</parameter>
		<parameter>
			<key>REDIRECT_URL_NO_JS_TESTPAGE</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/dynform_signing_testpage_eoss_action.do?method=ampss&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@&amp;mode=WAIT</value> 
		</parameter>
		<parameter>
			<key>REDIRECT_URL_NAME</key>
			<value>RedirectURL</value>
		</parameter>
		<parameter>
			<key>DATA_URL</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/eoss/normal?method=ampss&amp;mode=DATA&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@</value>
		</parameter>
		<parameter>
			<key>DATA_URL_TESTPAGE</key>
			<value>https://dgjustice-dss.arhs-developments.com/ejusticeportal/eoss/testpage?method=ampss&amp;mode=DATA&amp;DSS_EOSS_TOKEN=@@EOSS_TOKEN@@</value>
		</parameter>
		<parameter>
			<key>DATA_URL_NAME</key>
			<value>DataURL</value>
		</parameter>
		<parameter>
			<key>XML_REQUEST_DATA_TEMPLATE</key>
			<value>ampss.data.template</value>
		</parameter>
		<parameter>
			<key>IGNORE_BAD_SIGNATURE_FORMAT</key>
			<value>TRUE</value>
		</parameter>
	</SigningMethod>

	


